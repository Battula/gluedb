<%- model_class = Protocols::X12::TransactionSetPremiumPayment -%>
<%t '.title', :default => model_class.model_name.human.titleize.pluralize %>
<div class="page-header">
  <%= render 'shared/breadcrumb', crumbs: [@edi_transaction_set_payment.id] %>
</div>

<div class="row">
  <div class="col-md-5">
    <dl class="dl-horizontal">
    <%= display_aliased_fields(model_class, @edi_transaction_set_payment,{
      remove: ["id","carrier","transmission"],
      add: ["transaction_kind","payment_at"],
      replace:{"ts_id" => "Transaction type",
               "ts_control_number" => "Control number",
               "ts_implementation_convention_reference"=>"Reference standard"}
               }) %>
    </dl>
  </div>

  <div class="col-md-7">
    <div class="panel panel-default">
      <div class="panel-heading">
        <h3 class="panel-title">
        <span class="fui-document">&nbsp;</span>
          EDI Payload
        </h3>
      </div>
      <div class="panel-body">
        <pre><small><%= @edi_transaction_set_payment.body.read %></small></pre>
      </div>
    </div>
  </div>
</div>


<div class="row">
  <div class="col-md-4 col-md-offset-1">
    <div class="form-actions">

    </div>
  </div>
</div>


  <p class="text-right"><small><%= "updated: #{@edi_transaction_set_payment.updated_at.strftime('%m-%d-%Y') if @edi_transaction_set_payment.updated_at.present?}" %></small></p>

  <div class="well well-sm default">
    <div class="form-actions">

    </div>
  </div>
