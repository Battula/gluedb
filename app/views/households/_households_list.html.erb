<table class="table table-condensed">
  <thead>
    <tr>
      <th>Subscriber Name</th>
      <th># Individuals</th>
      <th># SEPs</th>
      <th># Eligibilities</th>
    </tr>
  </thead>
  <tbody>
    <%= table_empty_to_human if households.count < 1 %>
    <% households.each do |h| %>
      <tr>
        <td>
          <%= link_to prepend_glyph_to_name(h.subscriber.full_name), household_path(h) %>
        </td>
        <td><%= h.people.count %></td>
        <td><%= h.special_enrollment_periods.count %></td>
        <td><%= h.eligibilities.count %></td>
      </tr>
    <% end %>
  </tbody>
</table>
