= form_tag(member_address_changes_path, { :multipart => true, :method => :post, :class => "form form-horizontal"}) do |f|
  .row
    .col-sm-offset-1.col-sm-8
      = file_field_tag "address_change_file", :class => "file"
  .row
    .col-sm-offset-1
      = submit_tag "Upload", :class => "btn btn-primary"
