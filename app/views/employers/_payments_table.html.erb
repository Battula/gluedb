<table class="table table-condensed">
  <thead>
    <tr>
      <th>Date</th>
      <th>Carrier</th>
      <th>Total Payment</th>
      <th>Action</th>
    </tr>
  </thead>
  <tbody>
    <%= table_empty_to_human if premium_payments.count < 1 %>
    <% prev = nil %>
    <% premium_payments.each do |p| %>
      <tr>
        <td>
          <% unless p["_id"]["paid_at"] == prev %>
            <span class="fui-credit-card">&nbsp;</span>
            <%= p["_id"]["paid_at"].strftime("%m-%d-%Y") %>
          <% end %>
        </td>
        <td><%= p["carrier_name"] %></td>
        <td><%= number_to_currency (Rational( p["payment_amount"] ) / Rational(100)) %></td>
        <td><%= link_to "View detail", edi_transaction_set_payment_path(p["_id"]["transaction_set_premium_payment"]) %></td>
      </tr>
      <% prev = p["_id"]["paid_at"] %>
    <% end %>
  </tbody>
</table>
