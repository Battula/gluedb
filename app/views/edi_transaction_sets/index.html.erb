<%- model_class = Protocols::X12::TransactionSetEnrollment -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.pluralize.titleize %></h1>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:id) %></th>
      <th><%= model_class.human_attribute_name(:st01) %></th>
      <th><%= model_class.human_attribute_name(:st02) %></th>
      <th><%= model_class.human_attribute_name(:st03) %></th>
      <th><%= model_class.human_attribute_name(:bgn01) %></th>
      <th><%= model_class.human_attribute_name(:bgn02) %></th>
      <th><%= model_class.human_attribute_name(:bgn03) %></th>
      <th><%= model_class.human_attribute_name(:bgn04) %></th>
      <th><%= model_class.human_attribute_name(:bgn05) %></th>
      <th><%= model_class.human_attribute_name(:bgn06) %></th>
      <th><%= model_class.human_attribute_name(:bgn08) %></th>
      <th><%= model_class.human_attribute_name(:transaction_kind) %></th>
      <th><%= model_class.human_attribute_name(:submitted_at) %></th>
      <th><%= model_class.human_attribute_name(:aasm_state) %></th>
      <th><%= model_class.human_attribute_name(:ack_nak_processed_at) %></th>
      <th><%= model_class.human_attribute_name(:enrollment_id) %></th>
      <th><%= model_class.human_attribute_name(:edi_transmission_id) %></th>
      <th><%= model_class.human_attribute_name(:created_at) %></th>
      <th><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% @edi_transaction_sets.each do |edi_transaction_set| %>
      <tr>
        <td><%= link_to edi_transaction_set.id, edi_transaction_set_path(edi_transaction_set) %></td>
        <td><%= edi_transaction_set.st01 %></td>
        <td><%= edi_transaction_set.st02 %></td>
        <td><%= edi_transaction_set.st03 %></td>
        <td><%= edi_transaction_set.bgn01 %></td>
        <td><%= edi_transaction_set.bgn02 %></td>
        <td><%= edi_transaction_set.bgn03 %></td>
        <td><%= edi_transaction_set.bgn04 %></td>
        <td><%= edi_transaction_set.bgn05 %></td>
        <td><%= edi_transaction_set.bgn06 %></td>
        <td><%= edi_transaction_set.bgn08 %></td>
        <td><%= edi_transaction_set.transaction_kind %></td>
        <td><%= edi_transaction_set.submitted_at %></td>
        <td><%= edi_transaction_set.aasm_state %></td>
        <td><%= edi_transaction_set.ack_nak_processed_at %></td>
        <td><%= edi_transaction_set.enrollment_id %></td>
        <td><%= edi_transaction_set.transmission_id %></td>
        <td><%=l edi_transaction_set.created_at %></td>
        <td>
          <%= link_to t('.edit', :default => t("helpers.links.edit")),
                      edit_edi_transaction_set_path(edi_transaction_set), :class => 'btn btn-mini' %>
          <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
                      edi_transaction_set_path(edi_transaction_set),
                      :method => :delete,
                      :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
                      :class => 'btn btn-mini btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to t('.new', :default => t("helpers.links.new")),
            new_edi_transaction_set_path,
            :class => 'btn btn-primary' %>
